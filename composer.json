{
  "name": "it-devgroup/laravel-google-cloud-storage",
  "description": "A Google Cloud Storage filesystem for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Anatoly Bobrishov, it-devgroup.com",
      "email": "anatoly.bobrishov@it-devgroup.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "^5.1|^5.2|^5.3",
    "it-devgroup/flysystem-google-storage": "7.2.2",
    "illuminate/filesystem": "^5.1|^5.2|^5.3",
    "league/flysystem-cached-adapter": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ItDevgroup\\LaravelGoogleCloudStorage\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "ItDevgroup\\LaravelGoogleCloudStorage\\GoogleCloudStorageServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8"
  }
}
