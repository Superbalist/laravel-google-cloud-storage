{
  "name": "wfhq/laravel-google-cloud-storage",
  "description": "A Google Cloud Storage filesystem",
  "license": "MIT",
  "authors": [
    {
      "name": "The Force That Drives Business Forward",
      "email": "support@webforcehq.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "^5.1|^6.0|^7.0",
    "superbalist/flysystem-google-storage": ">=3.0 <8.0",
    "illuminate/filesystem": "^5.1|^6.0|^7.0",
    "league/flysystem-cached-adapter": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "wfhq\\LaravelGoogleCloudStorage\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "wfhq\\LaravelGoogleCloudStorage\\GoogleCloudStorageServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8"
  }
}
